{
  "name": "placenames",
  "homepage": "http://insights.nzherald.co.nz/apps/nz-placenames/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "d3": "^4.13.0",
    "d3-dsv": "^1.0.8",
    "deck.gl": "^5.1.1",
    "lodash": "^4.17.5",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-map-gl": "^3.2.4",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm-run-all build-js embed",
    "build-js": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "embed": "./embed.py",
    "deploy": "aws --region ap-southeast-2 s3 cp build/ s3://s3.newsapps.nz/apps/nz-placenames --acl=public-read --recursive --cache-control max-age=60,public",
    "dep": "npm-run-all clean build deploy",
    "clean": "rm -rf build"
  }
}
